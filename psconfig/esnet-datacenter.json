   "tests":{
      "test_tcp_throughput":{
         "type":"throughput",
         "spec":{
            "source":"{% address[0] %}",
            "dest":"{% address[1] %}",
            "duration":"PT60S",
            "ip-version": 6
         }
      },
      "test_tcp_throughput_reverse":{
         "type":"throughput",
         "spec":{
            "source":"{% address[1] %}",
            "dest":"{% address[0] %}",
            "duration":"PT60S",
            "ip-version": 6
         }
      },
      "test_latencybg":{
         "type":"latencybg",
         "spec":{
            "source":"{% address[0] %}",
            "dest":"{% address[1] %}",
            "flip":"{% flip %}",
            "packet-count" : 600,
            "packet-interval" : 0.1,
            "packet-padding" : 0,
            "ip-version": 6
         }
      },
      "test_trace":{
         "type":"trace",
         "spec":{
            "source":"{% address[0] %}",
            "dest":"{% address[1] %}",
            "ip-version": 6
         }
      }
   },
